{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": ["./objects", "./modules"],
    "includePattern": "\\.js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "template": "node_modules/tui-jsdoc-template",
    "encoding": "utf8",
    "destination": "docs/out/",
    "recurse": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "logo": {
      "url": "https://cdn.discordapp.com/attachments/939508059758288939/979289803239608360/FinalLogoAlt2.png",
      "width": "100px",
      "height": "100px",
      "link": "https://github.com/Project-Skill-Tree/Skill-Tree-Discord-Bot"
    }
  }
}
