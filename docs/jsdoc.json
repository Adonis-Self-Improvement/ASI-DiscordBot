{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": ["./objects", "./modules"],
    "includePattern": "\\.js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "template": "./template/tui.jsdoc-template",
    "encoding": "utf8",
    "destination": "docs/out/",
    "recurse": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}